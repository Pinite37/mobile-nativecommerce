{
  "expo": {
    "name": "AXI",
    "slug": "mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/axinotif.png",
    "scheme": "axiapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.axi.axiapp",
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/axiscreen.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.axi.axiapp",
      "googleServicesFile": "./google-services.json",
      "runtimeVersion": "1.0.0"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/axiscreen.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#121010"
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/axinotif.png",
          "color": "#121010",
          "sounds": []
        }
      ],
      "expo-secure-store",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "f5333952-d25c-444e-a7bc-11b81a5ac526"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/f5333952-d25c-444e-a7bc-11b81a5ac526"
    },
    "owner": "theophas"
  }
}
